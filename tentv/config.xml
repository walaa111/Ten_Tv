<?xml version='1.0' encoding='utf-8'?>
<widget id="com.realdealagency.ten2" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name> ten2</name>
    <description>
 ten2
    </description>
    <author email="ceo@realdealagency.com" href="http://www.realdealagency.com">
     ten2
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-inappbrowser" />
    <plugin name="admob" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="res/android/icon.png" />
        <icon density="mdpi" src="res/android/icon.png" />
        <icon density="hdpi" src="res/android/icon.png" />
        <icon density="xhdpi" src="res/android/icon.png" />
        <icon density="xxhdpi" src="res/android/icon.png" />
        <icon density="xxxhdpi" src="res/android/icon.png" />
    </platform>
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashScreen" value="screen" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="ShowSplashScreen" value="true" />
    <preference name="SplashScreenDelay" value="30000" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="~6.3.0" />
</widget>
